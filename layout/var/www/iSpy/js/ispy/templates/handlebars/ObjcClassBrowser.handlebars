<h1 class="page-header">
    <i class="fa fa-fw fa-code"></i>
    Class Browser
</h1>
<div class="row">
    <div id="objc-class-list" class="list-group col-md-4">
        {{#each objc_classes}}
            <a href="#" class="list-group-item animated fadeIn">
                <h5 class="list-group-item-heading">
                    {{ name }} <i class="fa fa-fw fa-angle-right"></i>
                </h5>
            </a>
        {{else}}
            <p class="text-center">
                <i class="fa fa-fw fa-refresh fa-spin"></i>
                Loading ...
            </p>
        {{/each}}
    </div>
    <div id="" class="col-md-8">
    </div>
</div>